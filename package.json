{
  "name": "gatsbyjs-blog",
  "description": "Sample blog built with Gatsby.js using Prismic headless CMS",
  "author": "Prismic",
  "dependencies": {
    "gatsby": "^3.0.4",
    "gatsby-plugin-manifest": "^3.0.0",
    "gatsby-plugin-offline": "^4.0.0",
    "gatsby-plugin-react-helmet": "^4.0.0",
    "gatsby-plugin-sharp": "^3.0.1",
    "gatsby-source-filesystem": "^3.0.0",
    "gatsby-source-prismic": "^3.3.6",
    "gatsby-transformer-sharp": "^3.0.0",
    "prismic-reactjs": "^1.3.3",
    "prismic-richtext": "^1.0.3",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gatsby-plugin-postcss": "^4.4.0",
    "postcss": "^8.2.13",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.1.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "tailwind": "tailwindcss build src/styles/tailwind.css -o src/styles/global.css",
    "tailwind:watch": "tailwindcss build src/styles/tailwind.css -o src/styles/global.css --watch",
    "build": "gatsby build",
    "develop": "gatsby develop && tailwind:watch",
    "start": "npm run clean && npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/prismicio/gatsby-blog"
  }
}
